    "karma-webpack": "^2.0.4",
    "mocha": "^3.5.3",
    "prettier": "^1.7.0",
    "publish-please": "^2.3.1",
    "remap-istanbul": "^0.9.5",
    "rimraf": "^2.6.2",
    "sinon": "^3.2.1",
    "sinon-chai": "^2.13.0",
    "ts-loader": "^2.3.7",
    "ts-node": "^3.3.0",
